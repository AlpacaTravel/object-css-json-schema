    "length": {
      "type": "object",
      "properties": {
        "value": {
          "type": "integer",
          "default": "2"
        },
        "unit": {
          "type": "string",
          "enum": ["px", "em", "%"],
          "default": "px"
        }
      }
    },
    "time": {
      "type": "object",
      "properties": {
        "value": {
          "type": "integer",
          "default": "2"
        },
        "unit": {
          "type": "string",
          "enum": ["s", "ms"],
          "default": "s"
        }
      }
    },
    "overflow": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string",
          "enum": ["visible", "scroll", "hidden", "auto"],
          "required": true
        }
      }
    }
  },
  "type": "object",
